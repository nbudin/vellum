<h1>New project</h1>

<%= error_messages_for :project %>

<% form_for(:project, :url => projects_path) do |f| %>
  <p>
    <b>Name</b><br />
    <%= f.text_field :name %>
  </p>
  
  <p>
    <b>Template Schema</b><br />
    <%= select_tag :template_schema, options_for_select(
	    TemplateSchema.find(:all).collect { |s| [ s.name, s.id ]}) %>
  </p>

  <p>
    <%= submit_tag "Create" %>
  </p>
<% end %>

<%= link_to 'Back', projects_path %>